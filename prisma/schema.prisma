

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://pguser:pgpassword@localhost:5432/mydb?schema=public"
}

model OntData {
  id      Int    @id @default(autoincrement())
  slot    Int
  port    Int
  ont_id  String
  sn      String @unique
  state   String
  origin  String 
}

